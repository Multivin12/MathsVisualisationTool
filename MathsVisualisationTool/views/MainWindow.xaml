<Window x:Class="MathsVisualisationTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MathsVisualisationTool"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="SolveIt Interpreter"    
        Background="{StaticResource MainColour_MainBackground}"
        
        Width="1209.104"
        Height="1021.726"
        WindowState="Maximized"      
        WindowStyle="none"
        Icon="../images/icons/home.png">
    <!--Width="{Binding SystemParameters.Primar964yScreenWidth}"-->
    <!--Height="{Binding SystemParameters.PrimaryScreenHeight}"-->

    <!--Width="1209.104" 
        Height="1021.726"-->

    <!--ThreeDBorderWindow - ADDS TOP TITLE BAR TO WINDOWSTYLE -->
    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="25"/>
    </WindowChrome.WindowChrome>

    <Window.CommandBindings>
        <CommandBinding Command="Cut" CanExecute="CommonCommandBinding_CanExecute"/>
        <CommandBinding Command="Copy" CanExecute="CommonCommandBinding_CanExecute"/>
        <CommandBinding Command="Paste" CanExecute="CommonCommandBinding_CanExecute"/>

        <CommandBinding Command="Undo" CanExecute="CommonCommandBinding_CanExecute"/>
        <CommandBinding Command="Redo" CanExecute="CommonCommandBinding_CanExecute"/>
    </Window.CommandBindings>

    <Grid>
        <DockPanel LastChildFill="True">
            <Grid VerticalAlignment="Top" Background="{StaticResource MainColour_SubBackground}" DockPanel.Dock="Top">
                <StackPanel WindowChrome.IsHitTestVisibleInChrome="True" VerticalAlignment="Center" HorizontalAlignment="Left" Orientation="Horizontal" Height="30">
                    <Image Source="../images/icons/SolveIT.png" Margin="2 2 0 0" HorizontalAlignment="Left"/>
                    <Label Content="SolveIT!" Foreground="{StaticResource MainColour_Font}" FontSize="16"/>
                </StackPanel>
                <StackPanel WindowChrome.IsHitTestVisibleInChrome="True" VerticalAlignment="Center" HorizontalAlignment="Right" Orientation="Horizontal" Height="30">
                    <Button Name="MiniBtn" Style="{StaticResource titleBtn_Style}" Margin="0 2 2 0" HorizontalAlignment="Right" Click="OnMinimise_Clicked">
                        <Image Source="../images/icons/mini.png" Height="20"/>
                    </Button>
                    <Button Name="MaxBtn" Style="{StaticResource titleBtn_Style}" Margin="0 2 2 0" HorizontalAlignment="Right" Click="OnResize_Clicked">
                        <Image Source="../images/icons/resize.png" Height="20"/>
                    </Button>
                    <Button Name="CloseBtn" ToolTip="Quit" Style="{StaticResource closeBtn_Style}" Margin="0 2 2 0" HorizontalAlignment="Right" Click="OnClose_Clicked">
                        <Image Source="../images/icons/close.png" Height="25"/>
                    </Button>
                </StackPanel>
            </Grid> 

            <!-- MAIN MENU/NAVIGATION MENU-->
            <Menu DockPanel.Dock="Top" IsMainMenu="True" >
                <!--<MenuItem VerticalAlignment="Center">
                    <MenuItem.Icon>
                        <Image Source="../images/icons/SolveIT.png" Width="30" Margin="0 0 -20 0"/>
                    </MenuItem.Icon>
                </MenuItem>-->
                <!--MAIN TOP MENU - FILE DROP DOWN-->
                <MenuItem Header="_File" VerticalAlignment="Center">
                    <MenuItem Header="_New" ToolTip="New Workshop" Click="OnNew_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/newFile.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="_Open..." ToolTip="Open saved SolveIT file">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/openFolder.png" Height="20"/>
                        </MenuItem.Icon>
                        <!-- OPEN PREVIOUSLY SAVED VARIABLES TABLE -->
                        <MenuItem Header="Open Variable File" ToolTip="Open saved variables" Click="OnOpenVar_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/openVar.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                        <!-- OPEN PREVIOUSLY SAVED WORKSHOP FILE -->
                        <MenuItem Header="Open Workshop File" ToolTip="Open saved variables" Click="OnOpenNum_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/openNum.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                    </MenuItem>
                    <Separator/>
                    <MenuItem Header="_Save As..." ToolTip="Save">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/saveAs.png" Height="20"/>
                        </MenuItem.Icon>
                        <MenuItem Header="Save Workshop As" Click="OnSave_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/saveDoc.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                        <MenuItem Header="Save Canvas Graph As" Click="OnSaveCanvas_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/saveGraph.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                        <MenuItem Header="Save Live Charts Graph As" Click="OnSaveLVC_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/saveGraph.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                        <MenuItem Header="Save Variables As" Click="OnSaveVar_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/saveVar.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                    </MenuItem>
                    <MenuItem Header="Save All" Click="OnSaveAll_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/saveAll.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <Separator/>
                    <MenuItem Header="_Print..." ToolTip="Print">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/print.png" Height="20"/>
                        </MenuItem.Icon>
                        <MenuItem Header="Print" Click="OnPrint_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/print.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                        <MenuItem Header="Page Setup" Click="OnPageSetup_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/pageSetup.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                    </MenuItem>
                    <Separator/>
                    <MenuItem Header="E_xit" Click="OnExitMenu_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/quit.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>
                <!--MAIN TOP MENU - EDIT DROPDOWN-->
                <MenuItem Header="_Edit" VerticalAlignment="Center">
                    <MenuItem Header="Undo" ToolTip="Undo (Ctrl+Z)" Click="OnUndo_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/undo.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Redo" Click="OnRedo_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/redo.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <Separator/>
                    <MenuItem Header="Cut" Command="Cut">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/cut.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Copy" Command="Copy">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/copy.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Paste" Command="Paste">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/paste.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>
                <!--MAIN TOP MENU - VIEW DROPDOWN-->
                <MenuItem Header="_View" VerticalAlignment="Center">
                    <MenuItem Header="Go to...">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/goTo.png" Height="20"/>
                        </MenuItem.Icon>
                        <MenuItem Header="Go To... Workshop" Click="OnGoToWorkshop_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/document.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                        <MenuItem Header="Go To... Canvas Graph" Click="OnGoToCanvas_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/canvas.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                        <MenuItem Header="Go To... Live Charts Graph" Click="OnGoToLVC_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/LVC.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                    </MenuItem>
                <Separator/>
                    <MenuItem Header="Fullscreen" Name="maximiseMenuItem" Click="OnMaximise_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/maxi.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Restore Size" Click="OnResize_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/resize.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Minimize" Click="OnMinimise_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/mini.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>
                <!--MAIN TOP MENU - TOOLS DROPDOWN-->
                <MenuItem Header="_Tools" VerticalAlignment="Center">
                    <MenuItem Header="SolveIT Updates">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/downUpdates.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="SolveIT Package Library">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/downUpdates.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                <Separator/>
                    <MenuItem Header="SolveIT Options">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/settings.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>
                <!--MAIN TOP MENU - HELP DROPDOWN-->
                <MenuItem Header="_Help" VerticalAlignment="Center">
                    <MenuItem Header="Help Library">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/library.png" Height="20"/>
                        </MenuItem.Icon>
                        <MenuItem Header="Test Document" Click="OnTestDoc_Clicked">
                            <MenuItem.Icon>
                                <Image Source="../images/icons/book.png" Height="20"/>
                            </MenuItem.Icon>
                        </MenuItem>
                    </MenuItem>
                    <MenuItem Header="View Help" Click="OnHelp_Clicked">
                        <MenuItem.Icon>
                            <Image Source="../images/icons/lifebuoy.png" Height="20"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>
            </Menu>

            <!-- TOOL BAR MENU -->
            <ToolBarTray DockPanel.Dock="Top">
                <ToolBar DockPanel.Dock="Top">
                    <!-- BACK BUTTON -->
                    <Button x:Name="BackBtn" ToolBar.OverflowMode="Never" ToolTip="Navigate Back" Click="OnBack_Clicked">
                        <Image Source="../images/icons/back.png" Width="25"/>
                    </Button>
                    <!-- FOREWARD BUTTON -->
                    <Button x:Name="ForwardBtn" ToolBar.OverflowMode="Never" ToolTip="Navigate Forward" Click="OnForward_Clicked">
                        <Image Source="../images/icons/forward.png" Width="25"/>
                    </Button>
                <Separator/>
                    <!-- NEW BUTTON -->
                    <Button x:Name="newBtn" ToolBar.OverflowMode="Never" ToolTip="New SolveIT" Click="OnNew_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/newFile.png" Width="25"/>
                    </Button>
                    <!-- OPEN VARIABLES BUTTON -->
                    <Button x:Name="openVarBtn" ToolBar.OverflowMode="Never" ToolTip="Open Previous SolveIT Variables" Click="OnOpenVar_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/openVar.png" Width="25"/>
                    </Button>
                    <!-- OPEN WORKSHOP BUTTON -->
                    <Button x:Name="openNumBtn" ToolBar.OverflowMode="Never" ToolTip="Open Previous SolveIT Workshop" Click="OnOpenNum_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/openNum.png" Width="25"/>
                    </Button>
                    <Separator/>
                    <!-- SAVE WORKSHOP AS BUTTON -->
                    <Button x:Name="saveDocBtn" ToolBar.OverflowMode="Never" ToolTip="Save Workshop As" Click="OnSave_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/saveDoc.png" Width="25"/>
                    </Button>
                    <!-- SAVE CANVAS AS BUTTON -->
                    <Button x:Name="saveCanvasBtn" ToolBar.OverflowMode="Never" ToolTip="Save Canvas Graph As" Click="OnSaveCanvas_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/saveGraph.png" Width="25"/>
                    </Button>
                    <!-- SAVE LVC AS BUTTON -->
                    <Button x:Name="saveLVCBtn" ToolBar.OverflowMode="Never" ToolTip="Save Live Charts As" Click="OnSaveLVC_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/saveGraph.png" Width="25"/>
                    </Button>
                    <!-- SAVE VARIABLES AS BUTTON -->
                    <Button x:Name="saveVarBtn" ToolBar.OverflowMode="Never" ToolTip="Save Live Charts As" Click="OnSaveVar_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/saveVar.png" Width="25"/>
                    </Button>
                    <!-- SAVE ALL AS BUTTON -->
                    <Button x:Name="saveAllBtn" ToolBar.OverflowMode="Never" ToolTip="Save All As" Click="OnSaveAll_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/saveAll.png" Width="25"/>
                    </Button>
                <Separator/>
                    <!-- PRINT BUTTON -->
                    <Button x:Name="printBtn" ToolBar.OverflowMode="Never" ToolTip="Print SolveIT Solution" Click="OnPrint_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/print.png" Width="25"/>
                    </Button>
                    <!-- PAGE SETUP BUTTON -->
                    <Button x:Name="pageBtn" ToolBar.OverflowMode="Never" ToolTip="Print SolveIT Solution" Click="OnPageSetup_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/pageSetup.png" Width="25"/>
                    </Button>
                <Separator/>
                    <!-- CUT BUTTON -->
                    <Button x:Name="cutBtn" Command="Cut" ToolBar.OverflowMode="Never" ToolTip="Cut Selection to Windows Clipboard" Click="OnCut_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/cut.png" Width="25"/>
                    </Button>
                    <!-- COPY BUTTON -->
                    <Button x:Name="copyBtn" Command="Copy" ToolBar.OverflowMode="Never" ToolTip="Copy Selection to Windows Clipboard" Click="OnCopy_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/copy.png" Width="25"/>
                    </Button>
                    <!-- PASTE BUTTON -->
                    <Button x:Name="pasteBtn" Command="Paste" ToolBar.OverflowMode="Never" ToolTip="Paste From Windows Clipboard" Click="OnPaste_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/paste.png" Width="25"/>
                    </Button>
                    <!-- TRASH BUTTON -->
                    <Button x:Name="trashBtn" ToolBar.OverflowMode="Never" ToolTip="Delete Selected Output" Click="OnTrash_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/trash.png" Width="25"/>
                    </Button>
                    <Separator/>
                    <!-- UNDO BUTTON -->
                    <Button x:Name="undoBtn" Command="Undo" ToolBar.OverflowMode="Never" ToolTip="Undo (Ctrl + Z)" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/undo.png" Width="25"/>
                    </Button>
                    <!-- REDO BUTTON -->
                    <Button x:Name="redoBtn" Command="Redo" ToolBar.OverflowMode="Never" ToolTip="Redo (Ctrl + Y)" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/redo.png" Width="25"/>
                    </Button>
                <Separator/>
                    <!-- RUN/SUBMIT BUTTON -->
                    <Button x:Name="altRunBtn" ToolTip="Run Input" ToolBar.OverflowMode="Never" Click="OnRun_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/run.png" Width="25"/>
                    </Button>
                    <!-- STAND ALONE KEYPAD BUTTON -->
                    <Button x:Name="keypadBtn" ToolTip="Keypad" ToolBar.OverflowMode="Never" Click="OnKeypad_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/calculator.png" Width="25"/>
                    </Button>
                    <Separator/>
                    <!-- NONE FUNCTIONAL COMBO/DROP DOWN BOX -->
                    <ComboBox x:Name="graphComboBox" Style="{StaticResource comboBox_Style}" Width="130" ToolBar.OverflowMode="Never" Padding="3 2 3 2">
                        <ComboBoxItem Content="-- Select Chart --" IsSelected="True"/>
                        <ComboBoxItem Content="Canvas Graph"/>
                        <ComboBoxItem Content="Live Charts"/>
                    </ComboBox>
                    <!-- OPEN GRAPH BUTTON -->
                    <Button x:Name="graphSelectBtn" ToolTip="Graph" ToolBar.OverflowMode="Never" Click="OnGraph_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/lineChart.png" Width="25"/>
                    </Button>
                    <Separator/>
                    <!-- SETTINGS BUTTON -->
                    <Button x:Name="SettingsBtn" ToolBar.OverflowMode="Never" ToolTip="SolveIT Settings" Click="OnSettings_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/settings.png" Width="25"/>
                    </Button>
                    <!-- HELP BUTTON -->
                    <Button x:Name="HelpBtn" ToolBar.OverflowMode="Never" ToolTip="SolveIT Help Library" Click="OnHelp_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/lifebuoy.png" Width="25"/>
                    </Button>
                    <!-- UPDATE BUTTON -->
                    <Button x:Name="UpdateBtn" ToolBar.OverflowMode="Never" ToolTip="SolveIT Updates" Click="OnUpdate_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/downUpdates.png" Width="25"/>
                    </Button>
                <Separator/>
                    <TextBox x:Name="searchBox" Style="{StaticResource searchTextBoxStyle}"></TextBox>
                    <Button x:Name="SearchBtn" ToolBar.OverflowMode="Never" ToolTip="Search Your Outputs" Click="OnSearch_Clicked" Style="{StaticResource Btn_ToolBar}">
                        <Image Source="../images/icons/search.png" Width="25"/>
                    </Button>
                </ToolBar>
            </ToolBarTray>

            <!-- BOTTOM SECTION OF DOCKPANEL -->
            <!-- Although not the main bottom section of the interpreter
            this section contains a status bar for the interpreter-->
            <StatusBar Name="statBar" DockPanel.Dock="Bottom" Background="LightGray" VerticalAlignment="Bottom" Margin="0 3 0 0">
                <StatusBarItem>
                    <TextBlock FontSize="10">Ready...</TextBlock>
                </StatusBarItem>
            </StatusBar>

            <!-- TOP SECTION OF DOCK PANEL (EXCLUDING MAIN MENU)-->
            <!-- This is the main section of where any outputs will be 
            displayed. Possible introduce a tab system to view different
            versions of an output i.e. graphical to numerical-->
            <Grid x:Name="topDock" DockPanel.Dock="Top" Margin="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition x:Name="Left_Column" Width="350" MinWidth="150"/>
                    <ColumnDefinition x:Name="LeftGridSplitter_Column" Width="7"/>
                    <ColumnDefinition x:Name="Centre_Column" Width="*"/>
                    <ColumnDefinition x:Name="RightGridSplitter_Column" Width="7"/>
                    <ColumnDefinition x:Name="Right_Column" Width="305" MinWidth="290"/>
                </Grid.ColumnDefinitions>
                <!-- LEFT COLUMN OF CENTRE GRID-->
                <!-- Left Section to include a Variable Table
                and links to documentation relevant to the interpreter-->
                <Grid Name="leftGrid" Margin="3 3 3 3" Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="7"/>
                        <RowDefinition Height="*" MinHeight="320"/>
                    </Grid.RowDefinitions>

                    <Grid Name="varGrid" Margin="3" Grid.Row="0" MaxHeight="270" VerticalAlignment="Top">
                        <DataGrid Name="varTable" AutoGenerateColumns="True" ColumnWidth="0.5*" Grid.Row="0" Style="{StaticResource DataGrid_Style}" IsReadOnly="True" RowHeaderWidth="0"/>
                    </Grid>

                    <!--</DockPanel>-->
                    <!-- *********************************************************************************************************** -->
                    <GridSplitter x:Name="Left2Mid_Splitter" Grid.Row="1" Style="{StaticResource GrdSpltrHorizontal_Style}"/>
                    <!-- *********************************************************************************************************** -->

                    <!-- *********************************************************************************************************** -->
                    <Grid Name="bottomLeftGrid" Grid.IsSharedSizeScope="True" Grid.Row="2" Grid.Column="0" Margin="0 3 0 0">
                        <TabControl x:Name="BottomLeft_TabMenu"  TabStripPlacement="Left" Style="{StaticResource mainTabControlStyle}">
                            <TabItem Style="{StaticResource LeftTab}">
                                <TabItem.Header>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="Notes" FontSize="15" Margin="0 0 0 3" Padding="5"/>
                                        <Image Source="../images/tabIcons/tabNotes90.png" Height="25"/>
                                    </StackPanel>
                                </TabItem.Header>
                                <Grid x:Name="NotesGrid" >
                                    <Grid Background="#5E5E5E" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="16"/>
                                            <RowDefinition Height="16"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="47*"/>
                                            <RowDefinition Height="328*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Style="{StaticResource leftTabStackPanel_Style}"  Grid.Column="0" Grid.RowSpan="5"/>
                                        <TextBlock Padding="8" Grid.Row="0" TextTrimming="CharacterEllipsis" Foreground="White" Text="Your Notes" Grid.RowSpan="2" Grid.Column="1"/>
                                        <ToolBarTray Grid.Row="2" Grid.Column="1">
                                            <ToolBar>
                                                <Button Name="notesNew" ToolBar.OverflowMode="Never" ToolTip="Add Note" Click="OnNewNote_Clicked">
                                                    <Image Source="../images/icons/newFile.png" Width="20"/>
                                                </Button>
                                                <Button Name="notesCut" ToolBar.OverflowMode="Never" ToolTip="Cut" Click="OnCut_Clicked">
                                                    <Image Source="../images/icons/cut.png" Width="20"/>
                                                </Button>
                                                <Button x:Name="notesCopy" ToolBar.OverflowMode="Never" ToolTip="Cut" Click="OnCopy_Clicked">
                                                    <Image Source="../images/icons/copy.png" Width="20"/>
                                                </Button>
                                                <Button x:Name="notesPaste" ToolBar.OverflowMode="Never" ToolTip="Cut" Click="OnPaste_Clicked">
                                                    <Image Source="../images/icons/paste.png" Width="20"/>
                                                </Button>
                                            </ToolBar>
                                        </ToolBarTray>
                                        <TreeView Grid.Row="3" Grid.RowSpan="2" Grid.Column="1">
                                            <TreeViewItem Header="Note 1">
                                                <TreeViewItem Header="Note 1 - Subject"/>
                                            </TreeViewItem>
                                            <TreeViewItem Header="Note 2">
                                                <TreeViewItem Header="Note 2 - Subject"/>
                                            </TreeViewItem>
                                        </TreeView>
                                    </Grid>
                                </Grid>
                            </TabItem>
                            <TabItem Style="{StaticResource LeftTab}">
                                <TabItem.Header>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="Library" FontSize="15" Margin="0 0 0 3" Padding="5"/>
                                        <Image Source="../images/tabIcons/tabDocs90.png" Height="25"/>
                                    </StackPanel>
                                </TabItem.Header>
                                <!--<StackPanel Background="{StaticResource BackgroundTrue_TabItem}" Margin="-3 -3 0 0" Width="5"  />-->
                                <Grid x:Name="DocsGrid" Grid.Column="0" Background="#5E5E5E" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Style="{StaticResource leftTabStackPanel_Style}" Grid.Column="0" Grid.RowSpan="2"/>
                                    <TextBlock Padding="8" TextTrimming="CharacterEllipsis" Foreground="White" Text="Documentation" Grid.Row="0" Grid.Column="1"/>
                                    <DocumentViewer Name="mainDocViewer" Padding="10" Grid.Row="1" Grid.Column="1"/>
                                </Grid>
                            </TabItem>
                        </TabControl>
                    </Grid>
                </Grid>

                <!-- *********************************************************************************************************** -->
                <GridSplitter x:Name="LeftGridSplitter" Style="{StaticResource GrdSpltrVertical_Style}" Grid.Column="1"/>
                <!-- *********************************************************************************************************** -->
                <Grid Name="centreGrid" Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="10"/>
                        <RowDefinition Height="60" MinHeight="60"/>
                    </Grid.RowDefinitions>
                    <Grid x:Name="mainOutputGrid" Margin="3" Grid.Row="0">
                        <TabControl x:Name="outputTabCon" Style="{StaticResource mainTabControlStyle}">
                            <TabItem Header="Numerical Workshop" ToolTip="SolveIT Numerical Workshop Display" Style="{StaticResource mainTab}">
                                <DockPanel x:Name="numWorkDock"  LastChildFill="True">
                                    <StackPanel DockPanel.Dock="Top" Style="{StaticResource TabHeaderStackPanel_Style}"/>
                                    <ListBox x:Name="Results" Style="{StaticResource resultListBox_Style}" SelectionMode="Multiple" ScrollViewer.CanContentScroll="True">
                                        <ListBox.ContextMenu>
                                            <ContextMenu>
                                                <MenuItem Header="Cut" Command="Cut">
                                                    <MenuItem.Icon>
                                                        <Image Source="../images/icons/cut.png" Height="20"/>
                                                    </MenuItem.Icon>
                                                </MenuItem>
                                                <MenuItem Header="Copy" Command="Copy">
                                                    <MenuItem.Icon>
                                                        <Image Source="../images/icons/copy.png" Height="20"/>
                                                    </MenuItem.Icon>
                                                </MenuItem>
                                            </ContextMenu>
                                        </ListBox.ContextMenu>
                                    </ListBox>
                                </DockPanel>
                            </TabItem>
                            <TabItem Header="Graph - Canvas Chart" ToolTip="SolveIT Live Charts Display" Style="{StaticResource mainTab}">
                                <DockPanel x:Name="canvasDock"  LastChildFill="True" Background="{StaticResource MainColour_MainBackground}">
                                    <StackPanel DockPanel.Dock="Top" Style="{StaticResource TabHeaderStackPanel_Style}"/>
                                    <Canvas x:Name="graphCanvas" Background="{StaticResource MainColour_MainBackground}" Margin="10"
                                            Width="500" Height="500"
                                            VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                </DockPanel>
                            </TabItem>
                            <TabItem Header="Graph - Live Chart" ToolTip="SolveIT Live Charts Display" Style="{StaticResource mainTab}">
                                <DockPanel x:Name="lvcDock"  LastChildFill="True">
                                    <StackPanel DockPanel.Dock="Top" Style="{StaticResource TabHeaderStackPanel_Style}"/>
